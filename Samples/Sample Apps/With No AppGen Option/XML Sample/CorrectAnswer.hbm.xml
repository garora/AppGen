<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="MySampleApplication" namespace="MySampleApplication" xmlns="urn:nhibernate-mapping-2.2">
  <class name="CorrectAnswer" table="correct_answers" lazy="true" >
    <id name="Caid">
      <generator class="identity" />
      <column name="caid" sql-type="int" not-null="true" />
    </id>
    <property name="Aid">
      <column name="aid" sql-type="int" not-null="true" />
    </property>
  </class>
</hibernate-mapping>